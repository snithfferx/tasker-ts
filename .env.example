# Firebase Configuration
# Copy this file to .env and fill in your Firebase project values

# App Information
APP_NAME=Tasker
APP_DESCRIPTION=A powerful time tracking solution for professionals and teams
APP_VERSION=1.0.0
APP_AUTHOR=Your Name
APP_URL=http://localhost:8080

# Firebase Project Configuration
# Get these values from your Firebase Console > Project Settings > General
FIREBASE_API_KEY=your_api_key_here
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=1:123456789:web:your_app_id
FIREBASE_MEASUREMENT_ID=G-YOUR_MEASUREMENT_ID
